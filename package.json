{
  "name": "callie",
  "version": "1.0.0-dev",
  "description": "Twilio Video SDK Quick Start for JavaScript",
  "main": "index.js",
  "scripts": {
    "clean": "npm run clean:quickstart",
    "clean:quickstart": "rimraf public/twilio.js",
    "build": "npm run build:quickstart",
    "build:quickstart": "browserify src/twilio.js > public/index.js",
    "start": "npm run clean && npm run build && webpack -d && node server",
    "watch": "webpack-dev-server --hot",
    "listen": "node listen.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkao1/callie.git"
  },
  "keywords": [
    "twilio",
    "video",
    "chat",
    "ip",
    "real",
    "time",
    "diggity"
  ],
  "author": "Jkao1",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkao1/callie/issues"
  },
  "homepage": "https://github.com/jkao1/callie#readme",
  "dependencies": {
    "@google-cloud/speech": "^0.10.3",
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "lodash": "^4.17.4",
    "node-record-lpcm16": "^0.3.0",
    "prismjs": "^1.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "twilio": "^3.0.0",
    "twilio-video": "^1.0.0",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  },
  "devDependencies": {
    "browserify": "^14.3.0",
    "copyfiles": "^1.2.0",
    "rimraf": "^2.6.1"
  }
}
